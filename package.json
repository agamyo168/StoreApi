{
  "name": "storeapi",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node --env-file=.env dist/app.js",
    "dev": "nodemon src/app.ts",
    "format": "prettier --config ./.prettierrc --write \"src/**/*.{ts,js}\"",
    "lint": "eslint  \"src/**/*.{ts,js}\"",
    "lint:fix": "eslint  \"src/**/*.{ts,js}\" --fix",
    "jasmine": "jasmine",
    "jasmine:ts": "jasmine-ts",
    "test": "npm run build && npm run jasmine",
    "clean": "rm -rf dist",
    "watch": "tsc-watch --esModuleInterop src/app.ts --outDir ./dist",
    "migrate": "db-migrate --env test up && db-migrate up",
    "test:migrate": "NODE_ENV=test db-migrate --env test up && jasmine-ts && db-migrate db:drop test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^22.9.3",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "jasmine": "^5.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0"
  }
}

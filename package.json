{
  "name": "storeapi",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node --env-file=.env dist/app.js",
    "dev": "nodemon src/app.ts",
    "format": "prettier --config ./.prettierrc --write \"src/**/*.{ts,js}\"",
    "lint": "eslint  \"src/**/*.{ts,js}\"",
    "lint:fix": "eslint  \"src/**/*.{ts,js}\" --fix",
    "jasmine": "jasmine",
    "jasmine:ts": "jasmine-ts",
    "test": "npm run build && npm run jasmine",
    "clean": "rm -rf dist",
    "watch": "tsc-watch --esModuleInterop src/app.ts --outDir ./dist",
    "migrate": "npx db-migrate up",
    "migrate:down":"npx db-migrate down",
    "migrate:reset":"npx db-migrate reset",
    "test:migrate": "npx db-migrate reset && npx db-migrate up && npm run build && npm run jasmine && npx db-migrate reset"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^22.9.3",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "dotenv": "^16.4.5",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "jasmine": "^5.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "express": "^4.21.1",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1"
  }
}
